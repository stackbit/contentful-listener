{
  "name": "@stackbit/contentful-listener",
  "version": "0.1.0",
  "description": "Contentful's content listener based on Contentful's sync API",
  "main": "dist/index.js",
  "scripts": {
    "test": "eslint src && prettier --check src",
    "build": "rm -rf ./dist && tsc",
