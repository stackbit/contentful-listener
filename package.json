  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stackbit/contentful-listener/issues"
  },
  "homepage": "https://github.com/stackbit/contentful-listener#readme",
  "dependencies": {
    "contentful": "^9.1.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
